{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "commonjs",
    "outDir": "../build/server",
    "baseUrl": "./server",
    "sourceMap": true,
    "isolatedModules": false,
    "noImplicitAny": false,
    "noEmit": false
  },
  "include": ["server"]
}
